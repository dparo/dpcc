{
    let array = [
        15, 59, 61, 75, 12, 71,  5, 35, 44,
        6, 98, 17, 81, 56, 53, 31, 20, 11,
        45, 80,  8, 34, 71, 83, 64, 28,  3,
        88, 50, 48, 80,  5
    ];


    let len = 32;
    print("Un-sorted array\n");
    print(array);

    {
        for (let curr_size = 1; curr_size < len; curr_size = 2 * curr_size) {
            for (let left_start = 0; left_start < len - 1; left_start = left_start + 2 * curr_size) {
                let mid = len - 1;
                if ((left_start + curr_size - 1) < len - 1) {
                    mid = left_start + curr_size - 1;
                }
                let right_end = len - 1;
                if ((left_start + 2 * curr_size - 1) < len - 1) {
                    right_end = left_start + 2 * curr_size - 1;
                }


                {
                    let l = left_start;
                    let m = mid;
                    let r = right_end;
                    let n1 = m - l + 1;
                    let n2 = r - m;

                    let L: int[1024];
                    let R: int[1024];

                    // Copy to temp arrays
                    for (let i = 0; i < n1; i++) {
                        L[i] = array[l + i];
                    }
                    for (let i = 0; i < n2; i++) {
                        R[i] = array[m + 1 + i];
                    }


                    let i = 0;
                    let j = 0;
                    let k = l;
                    while (i < n1 && j < n2) {
                        if (!(L[i] > R[j])) {
                            array[k++] = L[i++];
                        } else {
                            array[k++] = R[j++];
                        }
                    }

                    while (i < n1) {
                        array[k++] = L[i++];
                    }
                    while (j < n2) {
                        array[k++] = R[j++];
                    }
                }
            }
        }
    }

    print("Sorted array\n");
    print(array);
}print("\n\nIterative Merge sort\n");
